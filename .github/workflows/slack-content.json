           {
            	"text": "A GitHub Action has succeeded",
            	"attachments": [{
            		"pretext": "A GitHub Action has succeeded",
            		"color": "good",
            		"fields": [{
            				"title": "Repository",
            				"short": false,
            				"value": "<https://github.com/${{ github.repository }}|${{ github.repository }}>"
            			},
            			{
            				"title": "Ref",
            				"short": false,
            				"value": "${{ github.context.ref }}"
            			},
            			{
            				"title": "Commit",
            				"short": false,
            				"value": "<https://github.com/${{ github.repository }}/commit/${{ github.sha }}|${{ github.sha }}>"
            			},
            			{
            				"title": "Author",
            				"short": false,
            				"value": "${{ github.event.commits[github.event.commits.length - 1].author.name }}<${{ github.event.commits[github.event.commits.length - 1].author.email }}>"
            			},
            			{
            				"title": "Message",
            				"short": false,
            				"value": "${{ github.event.commits[github.event.commits.length - 1].message }}"
            			},
            			{
            				"title": "Workflow",
            				"short": false,
            				"value": "< https: //github.com/${{ github.repository }}/actions/runs/${{ github.run_id }}|${{ github.context.workflow }}>"
            			}
            		]
            	}]
            }
